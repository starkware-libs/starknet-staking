{
  "name": "StarkWare WebApps Monorepo",
  "version": "0.0.0",
  "packageManager": "pnpm@9.7.0",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@manypkg/cli": "^0.21.4",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "eslint-config-base": "workspace:*",
    "npkill": "^0.10.0",
    "prettier": "^3.3.3",
    "sort-package-json": "^2.10.0",
    "turbo": "^2.5.5"
  },
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --parallel",
    "dev:apps": "node ./scripts/dev-apps.js",
    "format": "sort-package-json && prettier --write \"**.+(js|json|md|yaml)\" && turbo run format --parallel",
    "lint": "turbo run lint",
    "manypkg:check": "manypkg check || exit 0",
    "npkill": "npkill",
    "pack": "turbo run pack",
    "pipeline": "pipeline",
    "postinstall": "pnpm manypkg:check"
  },
  "pnpm": {
    "patchedDependencies": {},
    "overrides": {
      "era-contracts": "github:matter-labs/era-contracts#446d391d34bdb48255d5f8fef8a8248925fc98b9",
      "ethereumjs-abi": "https://registry.npmjs.org/ethereumjs-abi/-/ethereumjs-abi-0.6.8.tgz"
    },
    "packageExtensions": {
      "@hyperlane-xyz/core": {
        "dependencies": {
          "ethers": "^5.8.0"
        }
      }
    }
  },
  "engines": {
    "node": ">=20.14.0"
  },
  "private": true
}
